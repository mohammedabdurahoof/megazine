<section>
  <div class="container">
    <div class="row mt-4">
      <a href="/admin/add-post" class="btn btn-success ml-auto" style="margin-top: 45px; margin-bottom: 30px;">Add Post</a>
    </div>
    <table class="table mt-4 display mt-5" id="table_id">
      <thead>
        <tr>
          <th scope="col">no</th>
          <th scope="col">name</th>
          <th scope="col">Aouther</th>
          <th scope="col">Date</th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody>
        {{#each posts}}
        <tr>
          <th >{{count @index}}</th>
          <td><a href="/admin/edit-post/{{this._id}}">{{this.data.title}}</a></td>
          <td>{{this.data.aouther}}</td>
          <td>{{this.date}}</td>
          <td>
            <a href="/admin/delect-post?id={{this._id}}" class="btn btn-danger"
              onclick="return confirm('are you sure  {{this.data.title}}')">Delect</a>
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</section>

