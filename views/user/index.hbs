 <div class="site-section py-0">
      <div class="owl-carousel hero-slide owl-style">
        {{#each newAdded}}
        <div class="site-section">
          <div class="container">
            <div class="half-post-entry d-block d-lg-flex bg-light">
              <div class="img-bg" style="background-image: url('images/post/post-thumbnails/big_{{_id}}.jpg')"></div>
              <div class="contents">
                <span class="caption">New Added</span>
                <h2><a href="/readPost/{{this._id}}">{{data.title}}</a></h2>
                <p class="mb-3 limited-letter">{{data.article}}</p>
                
                <div class="post-meta">
                  <span class="d-block"><a href="/readPost/{{this._id}}">{{data.aouther}}</a> in <a href="/category/{{this.data.category}}">{{data.category}}</a></span>
                  <span class="date-read">{{date}} <span class="icon-star2"></span></span>
                </div>

              </div>
            </div>
          </div>
        </div>
        {{/each}}
        


      </div>
    </div>
  

    <div class="site-section">
      <div class="container">
        <div class="row">
          <div class="col-lg-8">
            <div class="row">
              <div class="col-12">
                <div class="section-title">
                  <h2>Recent</h2>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col-md-6">
                <div class="post-entry-1">
                  <a href="/readPost/{{recent._id}}"><img src="images/post/post-thumbnails/medium_{{recent._id}}.jpg" alt="Image" class="img-fluid"></a>
                  <h2><a href="/readPost/{{recent._id}}">{{recent.data.title}}</a></h2>
                  <p  class="limited-letter text-black"><a href="blog-single.html" class="text-black">{{recent.data.article}}</a></p>
                  <div class="post-meta"> 
                    <span class="d-block"><a href="/readPost/{{recent._id}}">{{recent.data.aouther}}</a> in <a href="/category/{{recent.data.category}}">{{recent.data.category}}</a></span>
                    <span class="date-read">{{recent.date}} <span class="icon-star2"></span></span>
                  </div>
                </div>
              </div>
              <div class="col-md-6">
              {{#each three}}
                <div class="post-entry-2 d-flex">
                  <div class="thumbnail" style="background-image: url('/images/post/post-thumbnails/small_{{this._id}}.jpg')"></div>
                  <div class="contents">
                    <h2><a href="/readPost/{{this._id}}">{{this.data.title}}</a></h2>
                    <div class="post-meta">
                      <span class="d-block"><a href="/readPost/{{this._id}}">{{this.data.aouther}}</a> in <a href="/category/{{this.data.category}}">{{this.data.category}}</a></span>
                      <span class="date-read">{{this.date}}<span class="icon-star2"></span></span>
                    </div>
                  </div>
                </div>
              {{/each}}
                
              </div>
            </div>
          </div>
          <div class="col-lg-4">
            <div class="section-title">
              <h2>Popular Posts</h2>
            </div>
            {{#each popularPosts}}
            <div class="trend-entry d-flex">
              <div class="number align-self-start">0{{count @index}}</div>
              <div class="trend-contents">
                <h2><a href="/readPost/{{this._id}}">{{this.data.title}}</a></h2>
                <div class="post-meta">
                  <span class="d-block"><a href="/readPost/{{this._id}}">{{this.data.aouther}}</a> in <a href="/category/{{this.data.category}}">{{this.data.category}}</a></span>
                  <span class="date-read">{{this.date}}<span class="icon-star2"></span></span>
                </div>
              </div>
            </div>
          {{/each}}
            
            
            <p>
              <a href="/allposts" class="more">See All posts <span class="icon-keyboard_arrow_right"></span></a>
            </p>

          </div>
        </div>
      </div>
    </div>
    <!-- END section -->

